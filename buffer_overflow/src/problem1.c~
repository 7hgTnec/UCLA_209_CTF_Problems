#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int fun1(char *argv[])
{
  char buf[BUFFER_P1];
  strcpy(buf, argv[1]);
  return 0;
}

int main(int argc, char *argv[])
{
  fprintf(stderr, "target file execuated\n");
  if (argc != 2)
    {
      fprintf(stderr, "requires argc equal to 2\n");
      exit(-1);
    }
  fun1(argv);
  return 0;
}
